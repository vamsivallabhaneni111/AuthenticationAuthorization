@{
    if (@User.Identity.IsAuthenticated)
    {
        <h1>Logged in as @User.Identity.Name</h1>
        @Html.ActionLink("Logout", "Logout", "Accounts")
    }
    else
    {
        @Html.ActionLink("Login", "Landing", "Home")
    }
}
<br />
<br />
<br />
@RenderBody()

@if (@User.Identity.IsAuthenticated)
{


    <table>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}
